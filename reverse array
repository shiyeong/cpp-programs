#include <iostream>
using namespace std;
int main()
{
  int i,j,n;
  cout<<"enter the size of array:";
  cin>>n;
  int number[n];
  for(i=0;i<n;i++)
  {
    cout<<"enter array"<<i<<" ";
    cin>>number[i];
  }
  cout<<"entered array is:";
  for(j=0;j<n;j++)
  {
    cout<<number[j]<<""<<endl;
  }
  cout<<endl;
  cout<<"reversed array is:";
  for(j=n-1;j>=0;j--)
    {
      cout<<number[j]<<""<<endl;
    }
  return 0;
}
